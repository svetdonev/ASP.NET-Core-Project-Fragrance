@model FragranceListingViewModel

<form method="post" asp-controller="Fragrances" asp-action="Delete" asp-route-fragranceId="@Model.Id">
    <h1 class="text-orange">WARNING !!!</h1>
    <h4 class="text-orange">Are you sure you want to delete this fragrance?</h4>
    <button type="submit"  class="btn btn-outline-warning mb-4">Delete</button>
    <a class="btn btn-outline-warning mb-4" asp-controller="Fragrances" asp-action="All">Cancel</a>
</form>
